{
  "name": "gulp-nunjucks-starter-kit",
  "version": "1.0.0",
  "description": "Инструмент для компонентного проектирования статических сайтов, как lending page так и много-страничных проектов.",
  "main": "gulpfile.mjs",
  "bin": {
    "kit-tools": "./cli/kit-tools.mjs"
  },
  "scripts": {
    "tool:link": "npm link",
    "dev": "gulp",
    "build": "gulp build --build",
    "build:archive:tgz": "gulp build --build && kit-tools archive --options tgz build",
    "build:archive:tar": "gulp build --build && kit-tools archive --options tar build",
    "build:archive:zip": "gulp build --build && kit-tools archive --options zip build"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "browserslist": {
    "stylesheets": [
      "> 0.1% in GB and not dead",
      "last 6 Chrome versions",
      "last 6 Firefox versions",
      "last 6 Edge versions",
      "last 2 Samsung versions",
      "Firefox ESR",
      "Safari >= 11",
      "iOS >= 11",
      "ie 11"
    ],
    "javascripts": [
      "supports es6-module"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ],
    "node": [
      "node 18"
    ]
  },
  "os": [
    "darwin",
    "linux",
    "win64",
    "win32"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Template-Craft/gulp-nunjucks-starter-kit.git"
  },
  "keywords": [
    "gulp",
    "nunjucks",
    "scss",
    "babel",
    "es6"
  ],
  "author": {
    "name": "NИ",
    "email": "galaxyrobotix@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Template-Craft/gulp-nunjucks-starter-kit/issues"
  },
  "homepage": "https://github.com/Template-Craft/gulp-nunjucks-starter-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@types/archiver": "^6.0.2",
    "@types/gulp": "^4.0.17",
    "@types/gulp-clean-css": "^4.3.4",
    "@types/gulp-concat": "^0.0.37",
    "@types/gulp-plumber": "^0.0.37",
    "@types/gulp-postcss": "^8.0.6",
    "@types/gulp-rename": "^2.0.6",
    "@types/node-notifier": "^8.0.5",
    "@types/webpack-stream": "^3.2.15",
    "@types/yargs": "^17.0.32",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "browser-sync": "^3.0.2",
    "chalk": "^5.3.0",
    "concat": "^1.0.3",
    "cssnano": "^7.0.2",
    "cssnano-preset-advanced": "^7.0.2",
    "del": "^7.1.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-es-x": "^7.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "gulp": "^5.0.0",
    "gulp-beautify": "^3.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^3.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^4.0.0",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^5.1.0",
    "node-notifier": "^10.0.1",
    "postcss": "^8.4.38",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.3.1",
    "sass": "^1.77.4",
    "webpack": "^5.91.0",
    "webpack-stream": "^7.0.0",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "rfs": "^10.0.0"
  }
}
